.projects 
	no-select()
	position absolute
	width 20%
	height 100%
	padding-top 50px
	left 80%
	background #2C2C2C
	color white
	min-width 200px
	
	.collaborators 
		background #222
		height 40%
		border 1px solid rgb(118, 117, 117)
		overflow-y auto
		padding 10px
		.collab-head
			padding-bottom 5px
			border-bottom 1px solid #444
	

	.project-area 
		height 100%
		padding 10px
		border-top: 1px solid #777;
	
	.file 
		font-size 15px
		cursor default
		
	.project 
		font-size 20px
		font-family cursive
		cursor pointer
		.file
			padding-left 19px
			display none

    
